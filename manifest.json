{
  "manifest_version": 3,
  "name": "Content Extractor",
  "version": "1.0",
  "description": "Extracts and displays the main content of web pages",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["Readability.js"],
    "matches": ["<all_urls>"]
  }]
} 